  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jasonnathan/llm-core.git"
  },
  "dependencies": {
    "@huggingface/agents": "^0.0.5",
    "@huggingface/hub": "^1.0.2",
    "@huggingface/inference": "^3.5.1",
    "@huggingface/tasks": "^0.17.3",
    "@huggingface/transformers": "^3.4.0",
